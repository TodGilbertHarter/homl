/: /SIGNINDIALOG
	<template id='signin-dialog'>
	<div slot='content' id='contents' style='width: 350px;'>
		<h1 class='dialogtitle'>Sign In to Giant Electronic Brain</h1>
		<div class='loginfield'><label for='logindialogemail'>Email:</label><input type='text' id='logindialogemail'></div>
      	<div class='loginfield'><label for='logindialogpassword'>Password:</label><input type='password' id='logindialogpassword'></div>
      	<div style='clear: both;'><p>To create a new account, fill in your email and password, then <a href="#signup">click here</a> to sign up.</p></div>
	</div>
	<button class='dialogbutton' id='logindialogsignin' slot='buttonbar'>Sign In</button>
	<button class='dialogbutton' id='logindialogcancel' slot='buttonbar'>cancel</button>
:/
/: SIGNINDIALOG/ </template> :/
/: /SIGNUPDIALOG
	<template id='signup-dialog'>
	<div slot='content' id='contents' style='width: 350px;'>
		<h1 class='dialogtitle'>Sign Up to Giant Electronic Brain</h1>
		<div class='loginfield'><label for='logindialogemail'>Email:</label><input type='text' id='logindialogemail'></div>
      	<div class='loginfield'><label for='logindialogpassword'>Password:</label><input type='password' id='logindialogpassword'></div>
      	<div style='clear: both;'><p>To create a new account, fill in your email and password, then click sign up.</p></div>
	</div>
	<button class='dialogbutton' id='logindialogsignin' slot='buttonbar'>Sign Up</button>
	<button class='dialogbutton' id='logindialogcancel' slot='buttonbar'>cancel</button>
